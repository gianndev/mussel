let number = -5

if number > 0 {
    println("{number} is positive.")
} else {
    if number < 0 {
        println("{number} is negative.")
    } else {
        println("The number is zero.")
    }
}

let numbers = [1, 2, 3, 4, 5]
for num in numbers {
    println("Number in loop: {num}")
}
